{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
           "description": "Id da roupa cadastrada"
        },
        "success": {
          "type": "boolean",
          "description": "Caso deu sucesso ou erro na criação"
        },
        "message": {
          "type": ["string","null"],
          "description": "Mensagem de erro"
        }
      },
      "required": [
        "id",
        "success"
      ]
    }
  ]
}